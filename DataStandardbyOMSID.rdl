-- query for concatenating required asset names

create table #temp2 (dsid int, requiredimages varchar(max))

insert into #temp2

	select main.datastandardsid,
		   main.requiredassetsname as "required images"
	from
		(
			select distinct st2.datastandardsid, 
				(
					select st1.requiredassetname + ',' as [text()]
					from (select datastandardsid, ra.requiredassetname
						  from [data standards].dbo.requiredassets ra 
							left join #temp1 t1 
								on ra.requiredassetname = t1.requiredassetname
						  where assettype = 'image'
						  group by 
							  datastandardsid,
							  ra.requiredassetname) st1
					where st1.datastandardsid = st2.datastandardsid
					order by st1.datastandardsid
					for xml path ('')
				) [requiredassetsname]
			from (select datastandardsid, ra.requiredassetname
				  from [data standards].dbo.requiredassets ra 
					left join #temp1 t1 
						on ra.requiredassetname = t1.requiredassetname
				  where assettype = 'image'
				  group by 
					  datastandardsid,
					  ra.requiredassetname) st2
		) [main]

-- required documents

create table #temp3 (dsid int, requireddocs varchar(max))

insert into #temp3

	select main.datastandardsid,
		   main.requiredassetsname as "required documents"
	from
		(
			select distinct st2.datastandardsid, 
				(
					select st1.requiredassetname + ',' as [text()]
					from (select datastandardsid, ra.requiredassetname
						  from [data standards].dbo.requiredassets ra 
						  left join #temp1 t1 
							on ra.requiredassetname = t1.requiredassetname
						  where assettype = 'doc'
						  group by 
							  datastandardsid,
							  ra.requiredassetname) st1
					where st1.datastandardsid = st2.datastandardsid
					order by st1.datastandardsid
					for xml path ('')
				) [requiredassetsname]
			from (select datastandardsid, ra.requiredassetname
				  from [data standards].dbo.requiredassets ra 
				  left join #temp1 t1 
					on ra.requiredassetname = t1.requiredassetname
				  where assettype = 'doc'
				  group by 
					  datastandardsid,
					  ra.requiredassetname) st2
		) [main]

-- data to join with main query

create table #main(
	datastandardsid int, 
	[# of images required] int, 
	[# of docs required] int, 
	[required images] varchar(max), 
	[required docs] varchar(max))

insert into #main

	select ra.datastandardsid,
		sum(case when assettype = 'image' then 1 else 0 end) [# of images required],
		sum(case when assettype = 'doc' then 1 else 0 end) [# of docs required],
		case when t2.[requiredimages] is null then 'none required' else t2.[requiredimages] end as [required images],
		case when t3.requireddocs is null then 'none required' else t3.[requireddocs] end as [required docs]
	from [data standards].dbo.requiredassets ra 
		left join #temp1 t1 
			on ra.requiredassetname = t1.requiredassetname
		left join #temp2 t2 
			on ra.datastandardsid = t2.dsid
		left join #temp3 t3 
			on ra.datastandardsid = t3.dsid
	group by 
		datastandardsid,
		t2.[requiredimages],
		t3.requireddocs
	order by ra.datastandardsid

-- main query

select distinct
	s10.product_name_120,
	--below statement formats productnameexample in list format:
	--  - 31-piece air tool kit
	--  - 6-pieces air chisel set
	--  - hose clamp (10-pack)
	--  - 10 oz. air tool cleaner
	--  - 3/8 in. regulator with gauge
	--  - 1/4 in. npt male tire chuck
	'- ' + 
	case 
		when productnameexample like '%' + char(13) + char(10) + char(13) + char(10) + '%' 
		then replace(
			productnameexample, 
			char(13) + char(10) + char(13) + char(10), char(13) + char(10) + '- ')
		when productnameexample like '%' + char(13) + char(10) + '%' 
		then replace(
			productnameexample, 
			char(13) + char(10), char(13) + char(10) + '- ')
		else productnameexample 
	end as [productnameexample],

	ds.filename,
	map.url,
	otds.[oracle path],
	otds.name, 
	s10.thd_dept, 
	s10.thd_class, 
	case 
		when otds.dsid is null 
		then 'node unmapped' 
		else otds.filename 
	end, 

	case 
		when otds.dsid is null 
		then null else 
			case when fe.productnameformula is null 
			then 'no product formula mapped' 
		else fe.productnameformula end
	end [product name formula],
	ds.minimumrequiredbullet,

	case 
		when otds.dsid is null 
		then null else
			case 
				when m.[# of images required] is null 
				then 0  
				else m.[# of images required] 
			end 
		end [# of images required],

	case 
		when otds.dsid is null 
		then null else
			case 
				when m.[# of docs required] is null 
				then 0 
				else m.[# of docs required] 
			end 
	end [# of docs required],

	case 
		when otds.dsid is null 
		then null else 
			case 
				when m.[required images] is null 
				then 'none required' 
				else m.[required images] 
			end 
	end [required images],

	case 
		when otds.dsid is null 
		then null else
			case 
				when m.[required docs] is null 
				then 'none required' 
				else m.[required docs] 
			end 
	end [required docs],

	ds.datelastupdate,

	'https://portal.homedepot.com/sites/onlinemerchopsteam/data%20standards/' 
	+ ds.category + '/' 
	+ ds.filename as [sharepoint url]

from step_mviews.dbo.[omsid to datastandards] otds

	left join step_mviews.dbo.scorecard10 s10 
		on otds.name = s10.omsid
	left join [data standards].dbo.collectioncategories cc 
		on (cc.guidid = otds.guidid) and (otds.dsid = cc.datastandardsid)
	left join [data standards].dbo.formulaexample fe 
		on (fe.sequence = cc.formulaexamplesequence) and (cc.datastandardsid = fe.datastandardsid)
	left join [data standards].dbo.datastandards ds 
		on (ds.dsid = otds.dsid)
	left join [dataanalysis].dbo.bxs_urlmapping map 
		on map.pdf = ds.filename
	left join #main m 
		on m.datastandardsid = ds.dsid

where omsid in (@omsidlist)

