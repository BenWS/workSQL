  <DataSets>
    <DataSet Name="DSChanges">
      <Query>
        <DataSourceName>DataStandards</DataSourceName>
        <CommandText>if object_id('TempDB..#temp', 'u') is not null
	drop table #temp

select distinct cc.guidID, requiredAssetName
into #temp
from [data standards].dbo.DataStandards_Change_history dsh
inner join [data standards].dbo.requiredAssets ra
	on dsh.DataStandardsID = ra.DataStandardsID
inner join [data standards].dbo.collectionCategories cc
	on cc.DataStandardsID = dsh.DataStandardsID
inner join [data standards].dbo.IDMCollectionSetup idm
					on idm.GUIDID = cc.guidID
where (change_type = 'REQ_ASSET_DELETE' or change_type = 'REQ_ASSET_NEW')
	and cast(changeDateTime as date) = cast(Dateadd(day,-1, Getdate()) as date)
	and requiredAssetName in ('Energy Guide', 'Installation Guide','Instructions / Assembly','Product Brochure','Replacement Part List','Specification','Specifications','Use and Care Manual','Warranty','WarrantyReplacement Part List')
	and branchLeaf = 'L'

-- row concatenation
Select Main.GUIDID,
       Main.Assets As "Required Assets"
From
    (
        Select distinct ST2.GUIDID, 
            (
                Select ST1.RequiredAssetName + ',' AS [text()]
                From (select * from #temp) ST1
                Where ST1.GUIDID = ST2.GUIDID
                ORDER BY ST1.GUIDID
                For XML PATH ('')
            ) [Assets]
        From (select * from #temp) ST2
    ) [Main]</CommandText>
        <Timeout>180</Timeout>
      </Query>
      <Fields>
        <Field Name="guidID">
          <DataField>GUIDID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Required_Assets">
          <DataField>Required Assets</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
