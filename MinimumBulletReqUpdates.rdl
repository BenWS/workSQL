  <DataSets>
    <DataSet Name="DSChanges">
      <Query>
        <DataSourceName>DataStandards</DataSourceName>
        <CommandText>-- select guidID, current Bullet Value, previous Bullet Value
-- where changes occurred yesterday

if object_id('tempDB..#current', 'u') is not null
	drop table #current

if object_id('tempDB..#prev', 'u') is not null
	drop table #prev

declare @dt datetime
set @dt = getdate()

-- getting dataStandards from today
select dsid, minimumRequiredBullet 
into #current
from datastandards_history
where datediff(dd,@dt,backupdatetime) = 0

--getting dataStandards from yesterday
select dsid, minimumRequiredBullet 
into #prev
from datastandards_history
where datediff(dd,@dt,backupdatetime) = -1


-- comparing today's dataStandards with yesterday's
select 
	guidID [GUID ID], 
	#current.minimumRequiredBullet [New Value], 
	#prev.minimumRequiredBullet [Prev Value]
from #current
	inner join #prev
		on #current.dsid = #prev.dsid
	inner join collectionCategories cc
		on cc.dataStandardsID = #current.dsid
where #current.minimumRequiredBullet &lt;&gt; #prev.minimumRequiredBullet</CommandText>
        <Timeout>180</Timeout>
      </Query>
      <Fields>
        <Field Name="GUID_ID">
          <DataField>GUID ID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="New_Value">
          <DataField>New Value</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Prev_Value">
          <DataField>Prev Value</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
