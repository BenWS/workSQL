  <DataSets>
    <DataSet Name="ItemDetails">
      <Query>
        <DataSourceName>DCADashboard</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DepClassSel">
            <Value>=Parameters!DepClassSel.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT

[PRODUCT OMS SKU],
[PRODUCT DESCRIPTION],
[PRODUCT VIEWS DEPT BAND],  
DEPT, 
CLASS_NAME, 
[PRODUCT IDM ACCOUNT NAME], 

-- SCORED ATTRIBUTES
[NUMBER OF ALT IMAGES],
[MFG_MODEL],
CASE WHEN [COLLECTION (SEGMENTS)] = 'No Collection SKUs' THEN 'No' ELSE 'Yes' END [In Collection],
CASE WHEN [ACCESSORY (SEGMENTS)] = 'Has Accessory' THEN 'Yes' ELSE 'No' END [Has Accessories],
CASE WHEN [cordinating (segments)] = 'No Cordinating SKUs' THEN 'No' ELSE 'Yes' END [In Coodinating Item Rel.],
CASE WHEN [HAS_A_REVIEW] = 1 THEN 'Yes' ELSE 'No' END [Has a Review],
CASE WHEN [SHOW AS SSKU ON WEB] = 'Y' THEN 'Yes' ELSE 'No' END [Super SKU'd?]

FROM [DCA DASHBOARD].DBO.[TRAN_TBL_SCORINGMULTIPLIER_SKU_SOURCE]

WHERE DEPT = (SUBSTRING(@DepClassSel, 2, 2)) AND CLASS_NUMBER = (SUBSTRING(@DepClassSel, 6, LEN(@DepClassSel)))</CommandText>
        <Timeout>180</Timeout>
      </Query>
      <Fields>
        <Field Name="PRODUCT_OMS_SKU">
          <DataField>PRODUCT OMS SKU</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PRODUCT_DESCRIPTION">
          <DataField>PRODUCT DESCRIPTION</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PRODUCT_VIEWS_DEPT_BAND">
          <DataField>PRODUCT VIEWS DEPT BAND</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DEPT">
          <DataField>DEPT</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CLASS_NAME">
          <DataField>CLASS_NAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PRODUCT_IDM_ACCOUNT_NAME">
          <DataField>PRODUCT IDM ACCOUNT NAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NUMBER_OF_ALT_IMAGES">
          <DataField>NUMBER OF ALT IMAGES</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MFG_MODEL">
          <DataField>MFG_MODEL</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="In_Collection">
          <DataField>In Collection</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Has_Accessories">
          <DataField>Has Accessories</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="In_Coodinating_Item_Rel_">
          <DataField>In Coodinating Item Rel.</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Has_a_Review">
          <DataField>Has a Review</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Super_SKU_d_">
          <DataField>Super SKU'd?</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DepClassSel">
      <Query>
        <DataSourceName>DCADashboard</DataSourceName>
        <CommandText>SELECT

'D' + DEPT + ' C' + CLASS_NUMBER

FROM [DCA DASHBOARD].DBO.TRAN_TBL_SCORINGMULTIPLIER_SKU_SOURCE 


WHERE 

DEPT = '26'

GROUP BY

DEPT,
CLASS_NUMBER

ORDER BY 

CAST(DEPT AS INT) ASC, CAST(CLASS_NUMBER AS INT) ASC</CommandText>
        <Timeout>180</Timeout>
      </Query>
      <Fields>
        <Field Name="ID">
          <DataField />
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Count">
