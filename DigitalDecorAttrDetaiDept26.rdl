        </QueryParameters>
        <CommandText>SELECT COUNT(*) FROM [DCA DASHBOARD].DBO.[TRAN_TBL_SCORINGMULTIPLIER_SKU_SOURCE] WHERE DEPT = (SUBSTRING(@DepClassSel, 2, 2)) AND CLASS_NUMBER = (SUBSTRING(@DepClassSel, 6, LEN(@DepClassSel)))</CommandText>
        <Timeout>180</Timeout>
      </Query>
      <Fields>
        <Field Name="ID">
          <DataField />
          <rd:TypeName>System.Int32</rd:TypeName>
