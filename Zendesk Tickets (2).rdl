    <DataSet Name="Open_Tickets">
      <Query>
        <DataSourceName>ZendeskReport</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@GroupName">
            <Value>=Parameters!GroupName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
    groupname,
    SUM(CASE WHEN status IN ('new', 'open') THEN 1 ELSE 0 END) AS [Total_Open],
    SUM(CASE WHEN status IN ('new', 'open') AND CAST(ticketcreateddate AS SMALLDATETIME) BETWEEN DATEADD(DAY, -5, GETDATE()) AND DATEADD(DAY, -3, GETDATE()) THEN 1 ELSE 0 END) AS [Tickets Older Than 3 Days],
    SUM(CASE WHEN status IN ('new', 'open') AND CAST(ticketcreateddate AS SMALLDATETIME) BETWEEN DATEADD(DAY, -10, GETDATE()) AND DATEADD(DAY, -5, GETDATE())  THEN 1 ELSE 0 END) AS [Tickets Older Than 5 Days],
    SUM(CASE WHEN status IN ('new', 'open') AND CAST(ticketcreateddate AS SMALLDATETIME) &lt; DATEADD(DAY, -10, GETDATE()) THEN 1 ELSE 0 END) AS [Tickets Older Than 10 Days]
FROM
    zendeskunclosedticketexport
WHERE
	groupname = @GroupName
GROUP BY
    groupname</CommandText>
        <Timeout>180</Timeout>
      </Query>
      <Fields>
        <Field Name="groupname">
          <DataField>groupname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Total_Open">
          <DataField>Total_Open</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Tickets_Older_Than_3_Days">
          <DataField>Tickets Older Than 3 Days</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Tickets_Older_Than_5_Days">
          <DataField>Tickets Older Than 5 Days</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="organization">
          <DataField>organization</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="Tickets_Older_Than_10_Days">
          <DataField>Tickets Older Than 10 Days</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ResolvedTickets">
      <Query>
        <DataSourceName>ZendeskReport</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="GroupName">
            <Value>=Parameters!GroupName.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- tickets closed

declare @beginningofyear as date 
declare @beginningoflw as date 
declare @endoflw as date 
declare @startDate as smalldatetime

set @beginningofyear = smalldatetimefromparts(year(getdate()), 1, 1, 0, 0) 
set @beginningoflw = dateadd(week, datediff(week, 0, getdate()) - 1, 0) 
set @endoflw = dateadd(week, 1, @beginningoflw) 
set @startDate =  -- get first ticketCreatedDate
	(select top 1 cast(ticketCreatedDate as date) 
	from idmZendesk.dbo.zendeskTicketExport
	where groupname = @groupName
	order by cast(TicketUpdatedDate as date) asc)

select 
	groupname, 
    sum(case 
			when cast (createtimestamp as smalldatetime) 
				between @beginningoflw and @endoflw 
			then 1 
			else 0 
		end) as 'resolved lw', 
       sum(case 
				when cast (createtimestamp as smalldatetime) 
					&gt; @beginningofyear 
				then 1 
				else 0 
			end) as 'resolved ytd', 
       sum(case 
			when groupName = @groupName 
				and cast(createtimestamp as smalldatetime) between @startDate and @endoflw 
				then 1
			else 0
		end) -- number of tickets logged
		/ ( ( datediff(week, @startDate, dateadd(week, -1, getdate())) ) + 1 ) -- divide by number of weeks
	   as 'average per week' 
from   zendeskticketexport 
where  groupname = @GroupName 
group  by groupname 
order  by 'resolved lw' desc, 
          'resolved ytd' desc </CommandText>
        <Timeout>180</Timeout>
      </Query>
      <Fields>
        <Field Name="groupname">
          <DataField>groupname</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="Resolved_LW">
          <DataField>resolved lw</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Resolved_YTD">
          <DataField>resolved ytd</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Average_Per_Week">
          <DataField>average per week</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="TopRequesters">
      <Query>
        <DataSourceName>ZendeskReport</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="groupname">
            <Value>=Parameters!GroupName.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @BeginningOfYear AS DATE 
DECLARE @BeginningOfLW AS DATE 
DECLARE @EndOfLW AS DATE 

SET @BeginningOfYear = Smalldatetimefromparts(Year(Getdate()), 1, 1, 0, 0) 
SET @BeginningOfLW = Dateadd(week, Datediff(week, 0, Getdate()) - 1, 0) 
SET @EndOfLW = Dateadd(week, Datediff(week, 0, Getdate()), 0) 

SELECT TOP 10 p.requestor, 
              p.organization, 
              p.internalgroup, 
              Sum(p.lw)  AS [LW], 
              Sum(p.ytd) AS [YTD] 
FROM   (SELECT groupname, 
               ze.requestor, 
               ze.organization, 
               ze.requestoremail, 
               bxs.internalgroup, 
               Sum(CASE WHEN Cast(ticketcreateddate AS SMALLDATETIME) BETWEEN @BeginningOfLW AND @EndOfLW THEN 1 ELSE 0 END) AS [LW], 
               Sum(CASE WHEN Cast(ticketcreateddate AS SMALLDATETIME) &gt; @BeginningOfYear THEN 1 ELSE 0 END) AS [YTD] 
        FROM   zendeskticketexport ze 
               LEFT JOIN [DataAnalysis].[dbo].[bxs_dc_internal_contacts_zd] bxs 
                      ON ze.requestoremail = bxs.emailaddress 
        WHERE  groupname = @GroupName 
        GROUP  BY ze.requestor, 
                  ze.organization, 
                  ze.requestoremail, 
                  bxs.internalgroup, 
                  ze.groupname 
        UNION 
        SELECT groupname, 
               zu.requestor, 
               zu.organization, 
               zu.requestoremail, 
               bxs.internalgroup, 
               Sum(CASE WHEN Cast(ticketcreateddate AS SMALLDATETIME) BETWEEN @BeginningOfLW AND @EndOfLW THEN 1 ELSE 0 END) AS [LW], 
               Sum(CASE WHEN Cast(ticketcreateddate AS SMALLDATETIME) &gt; @BeginningOfYear THEN 1 ELSE 0 END) AS [YTD] 
        FROM   zendeskunclosedticketexport zu 
               LEFT JOIN [DataAnalysis].[dbo].[bxs_dc_internal_contacts_zd] bxs 
                      ON zu.requestoremail = bxs.emailaddress 
        WHERE  groupname = @GroupName 
        GROUP  BY zu.requestor, 
                  zu.organization, 
                  zu.requestoremail, 
                  bxs.internalgroup, 
                  zu.groupname) AS p 
GROUP  BY p.requestor, 
          p.groupname, 
          p.organization, 
          p.internalgroup 
ORDER  BY lw DESC </CommandText>
        <Timeout>180</Timeout>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="requestor">
          <DataField>requestor</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LW">
          <DataField>LW</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="internalgroup">
          <DataField>internalgroup</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="organization">
          <DataField>organization</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="YTD">
          <DataField>YTD</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="TicketsLogged">
      <Query>
        <DataSourceName>ZendeskReport</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="GroupName">
            <Value>=Parameters!GroupName.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--tickets logged

declare @endoflw as date 
declare @beginningofyear as date 
declare @beginningoflw as date
declare @startDate as date

set @beginningofyear = smalldatetimefromparts(year(getdate()), 1, 1, 0, 0) 
set @beginningoflw = dateadd(week, datediff(week, 0, getdate()) - 1, 0) 
set @endoflw = dateadd(week, datediff(week, 0, getdate()), 0) 
set @startDate =  -- get first ticketCreatedDate
	(select top 1 cast(ticketCreatedDate as date) 
	from idmZendesk.dbo.zendeskTicketExport
	where groupname = @groupName
	order by cast(TicketUpdatedDate as date) asc)

select p.groupname, 
       sum(p.lw) as lw, 
       sum(p.ytd) as ytd, 
       sum(p.[average per week]) as 'average per week' 
from   (select groupname, 
                sum(case 
						when cast(ticketcreateddate as smalldatetime) 
							between @beginningoflw and @endoflw 
						then 1 
						else 0 
					end) as 'lw',
				sum(case 
						when cast(ticketcreateddate as smalldatetime) &gt; @beginningofyear 
						then 1 
						else 0 
					end) as 'ytd',
               sum(case 
					when groupName = @groupName 
						and cast(ticketCreatedDate as smalldatetime) between @startDate and @endoflw 
						then 1
					else 0
				end) --total number of tickets
				/ ( ( datediff(week, @startDate, dateadd(week, -1, getdate())) ) + 1 ) as 'average per week' --divided by week
from   zendeskticketexport 
where  groupname = @GroupName 
group  by groupname 

union 

select groupname, 
	sum(case 
			when cast(ticketcreateddate as smalldatetime) 
				between @beginningoflw and @endoflw 
			then 1 
			else 0 
		end) as 'lw',
	sum(case 
			when cast(ticketcreateddate as smalldatetime) &gt; @beginningofyear 
			then 1 
			else 0 
		end) as 'ytd',
    sum(case 
			when groupName = @groupName 
				and cast(ticketCreatedDate as smalldatetime) between @startDate and @endoflw 
				then 1
			else 0
		end) -- total number of tickets
		/ ( ( datediff(week, @startDate, dateadd(week, -1, getdate())) ) + 1 ) as 'average per week' --divided by week
from   zendeskunclosedticketexport 
where  groupname = @GroupName 
group  by groupname) as p 
group  by p.groupname </CommandText>
        <Timeout>180</Timeout>
      </Query>
      <Fields>
        <Field Name="groupname">
          <DataField>groupname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LW">
          <DataField>lw</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="YTD">
          <DataField>ytd</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Average_Per_Week">
          <DataField>average per week</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!groupname.Value</FilterExpression>
          <Operator>Equal</Operator>
          <FilterValues>
            <FilterValue>=Parameters!GroupName.Value</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
