    <DataSet Name="avgResTime">
      <Query>
        <DataSourceName>ZendeskReport</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@GroupName">
            <Value>=Parameters!GroupName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>if object_id ('tempDB..#resTime') is not null
	drop table #resTime

if object_id ('tempDB..#lw') is not null
	drop table #lw

if object_id ('tempDB..#ytd') is not null
	drop table #ytd
select 
		groupname, 
		requestoremail,
		-- column for time to solve
		-- case block 2: test if time to solve &lt; 1 day
		case 
			when 
			
				(-- case block 1: subtract a day from particular issue types
				case
					when (THD_RequestTypeIDMForm = 'fulfillment_method_change__chub_to_yow' 
						or THD_RequestType_DCA_OMS_Form = 'fulfillment_method_change__chub_to_yow') 
					then cast(FullResolutionTimeInMinutesBusiness as float)/60/8 - 1
					when (THD_RequestTypeIDMForm = 'item_relationships__collections' 
						or THD_RequestType_DCA_OMS_Form = 'item_relationships__collections') 
					then cast(FullResolutionTimeInMinutesBusiness as float)/60/8 - 1
					when (THD_RequestTypeIDMForm = 'attribute_update_requests' 
						or THD_RequestType_DCA_OMS_Form = 'attribute_update_requests') 
					then cast(FullResolutionTimeInMinutesBusiness as float)/60/8 - 1
				else cast(FullResolutionTimeInMinutesBusiness as float)/60/8 end) &lt; 0 then 0
			else
				(-- case block 1: subtract a day from particular issue types
				case
					when (THD_RequestTypeIDMForm = 'fulfillment_method_change__chub_to_yow' 
						or THD_RequestType_DCA_OMS_Form = 'fulfillment_method_change__chub_to_yow') 
					then cast(FullResolutionTimeInMinutesBusiness as float)/60/8 - 1
					when (THD_RequestTypeIDMForm = 'item_relationships__collections' 
						or THD_RequestType_DCA_OMS_Form = 'item_relationships__collections') 
					then cast(FullResolutionTimeInMinutesBusiness as float)/60/8 - 1
					when (THD_RequestTypeIDMForm = 'attribute_update_requests' 
						or THD_RequestType_DCA_OMS_Form = 'attribute_update_requests') 
					then cast(FullResolutionTimeInMinutesBusiness as float)/60/8 - 1
				else cast(FullResolutionTimeInMinutesBusiness as float)/60/8 end) 
			end as [resolutionTime],
			ticketCreatedDate
into #resTime
from idmzendesk.dbo.zendeskticketexport

-- average business days to solve - last week
select avg(resolutionTime) [Bus. Days to Solve], 'id' [id]
into #lw
from #resTime
where groupName = @GroupName
	and cast(ticketCreatedDate as date) &gt;= Dateadd(week, Datediff(week, 0, Getdate()) - 1, 0) -- bgn lw
	and cast(ticketCreatedDate as date) &lt;=  Dateadd(week, 1, Dateadd(week, Datediff(week, 0, Getdate()) - 1, 0)) -- end lw

-- average business days to solve -- year to date

select avg(resolutionTime) [Bus. Days to Solve], 'id' [id]
into #ytd
from #resTime
where groupName = @GroupName
	and cast(ticketCreatedDate as date) &gt;= smalldatetimefromparts(year(getdate()), 1, 1, 0, 0) -- bgn ly

select round(a.[Bus. Days to Solve], 2) as [Year], round(b.[Bus. Days to Solve], 2) [LW]
from #ytd a
	full outer join #lw b
		on b.[id] = a.[id]</CommandText>
      </Query>
      <Fields>
        <Field Name="Year">
          <DataField>Year</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="LW">
          <DataField>LW</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
